    <messages>
        <message id="236" name="ZHY_ORDER_COMMAND">
            <description>平台端向机巢或无人机下发命令使用，接收发执行后需要用Log接口返回执行结果，服务端到移动端参数用ZHY_ORDER_COMMAND接口传输</description>
            <field type="uint8_t" name="target_system">System which should execute the command</field>
            <field type="uint8_t" name="target_component">Component which should execute the command, 0 for all components</field>
            <field type="float" name="param1">pitch</field>
            <field type="float" name="param2">yaw</field>
            <field type="float" name="param3">roll</field>
            <field type="float" name="param4">verticalThrottle</field>
            <field type="uint8_t" name="param5" enum="MAV_CMD">action</field>
            <field type="uint8_t" name="param6">actionParam</field>
            <field type="uint8_t[240]" name="param7">data</field>
        </message>
        <message id="239" name="ZHY_OPERATION_LOG">
            <description>安卓执行指令返回结果信息</description>
            <field type="uint32_t" name="timestamp">时间戳</field>
            <field type="uint8_t" name="source_type" enum="SOURCE_TYPE">来源类型，See SOURCE_TYPE enum</field>
            <field type="uint8_t" name="action">执行指令</field>
            <field type="uint8_t" name="log_levels" enum="LOG_LEVEL">返回信息等级. See LOG_LEVEL enum</field>
            <field type="char[24]" name="time">时间</field>
            <field type="char[1]" name="is_success">执行结果: 1 success, 0 failed</field>
            <field type="char[170]" name="message">返回结果信息 根据action区别解析</field>
            <field type="char[36]" name="device_id">设备id</field>
        </message>
        <message id="240" name="ZHY_DEVICE_REGISTRATION">
            <description>安卓侧主动向平台发送注册信息，移动端到服务端参数接口用 ZHY_DEVICE_REGISTRATION 接口传输</description>
            <field type="uint8_t" name="device_type" enum="DEVICE_TYPE">device type; MSDK-android:1 OSDK:2，机巢:3. See DEVICE_TYPE enum</field>
            <field type="char[50]" name="device_id">Equipment unique identification code</field>
            <field type="char[80]" name="device_name">device or airCaft name</field>
            <field type="char[68]" name="aliases">别名 Device aliases user set,if any</field>
        </message>
    </messages>
</mavlink>
